    {
      "name": "profiles",
      "label": "Profils",
      "system": "coc",
      "path": "./packs/profiles.db",
      "type": "Item",
      "tag": "profile"
    },
    {
      "name": "traits",
      "label": "Traits",
      "system": "coc",
      "path": "./packs/traits.db",
